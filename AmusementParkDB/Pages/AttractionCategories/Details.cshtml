@page
@model AmusementParkDB.Pages.AttractionCategories.DetailsModel

<div>
    <h4>Attraction Category</h4>
    <hr />

    <dl class="row">
        <dt class="col-sm-2">Attraction Category Name</dt>
        <dd class="col-sm-10">@(Model.AttractionCategory.Name ?? "-")</dd>

        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@(Model.AttractionCategory.Description ?? "-")</dd>
    </dl>
</div>

<div>
    <a asp-page="./Edit" asp-route-id="@Model.AttractionCategory.Id">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
